{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for the application",
  "windows": ["main", "workspace-*"],
  "permissions": [
    "core:default",
    "core:window:allow-set-title",
    "dialog:allow-open",
    "dialog:allow-save",
    "updater:default",
    "process:allow-restart",
    "log:default",
    "opener:default"
  ]
}
